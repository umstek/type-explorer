{
  "name": "type-explorer",
  "displayName": "Type Explorer (Preview)",
  "description": "The missing object browser for vscode",
  "version": "0.1.0-alpha.1",
  "preview": true,
  "publisher": "YOUR_PUBLISHER_ID",
  "repository": {
    "type": "git",
    "url": "https://github.com/YOUR_USERNAME/type-explorer"
  },
  "engines": {
    "vscode": "^1.108.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "typeExplorer",
          "name": "Type Explorer"
        }
      ]
    },
    "commands": [
      {
        "command": "type-explorer.refresh",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "type-explorer.showDocumentation",
        "title": "Show Documentation"
      },
      {
        "command": "type-explorer.browsePackage",
        "title": "Browse Package...",
        "icon": "$(package)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "type-explorer.refresh",
          "when": "view == typeExplorer",
          "group": "navigation"
        },
        {
          "command": "type-explorer.browsePackage",
          "when": "view == typeExplorer",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.108.1",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "npm-run-all": "^4.1.5",
    "typescript-eslint": "^8.52.0"
  }
}
